{
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-param-reassign": "off",
    "global-require": "off",
    "import/no-dynamic-require": "off",
    "no-console": "off"
  }
}
